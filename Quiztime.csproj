<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>qt.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Heebo-VariableFont_wght.ttf" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="qt.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MySqlConnector" Version="2.1.9" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="fonts\Heebo.ttf">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
